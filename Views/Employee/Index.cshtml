@model IEnumerable<CrudOperatonApp.Models.Employee>

@{
    ViewData["Title"] = "Index";
}
<div class="container">

    <div class="row">

        <div class="col-8 offset-2">
            <h1 class="text-center text-primary">Employee Record</h1>
            <table class="table table-bordered">
                <tr class="text-center">
                    <th>EmployeeID</th>
                    <th>Name</th>
                    <th>City</th>
                    <th>State</th>
                    <th>Salary</th>

                </tr>
       @foreach(var d in Model)
            {
            <tr class="text-center">
                <td>@d.Id</td>
                <td>@d.Name</td>
                <td>@d.City</td>
                <td>@d.State</td>
                <td>@d.Salary</td>

            </tr>
            }
            </table>
        </div>

    </div>
</div>
